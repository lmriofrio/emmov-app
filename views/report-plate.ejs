<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="/images/favicon.ico" type="image/x-icon">
    <title>Reportes de placas</title>
    <link rel="stylesheet" href="css/style.css">
    <script src="node_modules/simplebar/dist/simplebar.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@2.46.0/tabler-icons.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.js"
        integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
    
    <script src="/js/activar-items-navbar.js"></script>
    <script src="/js/spinners.js"></script>
    <script src="../node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

</head>

<body>

    <div class="page-wrapper" id="main-wrapper" data-theme="blue_theme" data-layout="vertical" data-sidebartype="full"
        data-sidebar-position="fixed" data-header-position="fixed">
        <!-- Inicio de barra de navegación -->

        <!--  Fin de barra de navegación -->
        <!--  Contendio principal -->
        <div class="body-wrapper">
            <!--  Encabezado de pagina -->
            <header class="app-header">
                <nav class="navbar navbar-expand-lg navbar-light">
                </nav>
            </header>
            <!--  Fin de encabezado de pagina  -->
            <div class="container-fluid">
                <div class="widget-content searchable-container list">
                    <div class="card card-body">

                        <div class="d-sm-flex d-block align-items-center justify-content-between mb-9">
                            <div class="mb-3 mb-sm-0">
                                <h5 class="card-title fw-semibold">Reporte de pagos para la elaboración de placas</h5>
                                <p class="card-subtitle mb-0">Información de la recaudación de pagos de placas</p>
                            </div>
                            <div style="width: 20%;">
                                <input type="text" name="" class="form-control-disable text-center" id=""
                                    placeholder="">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <label for="exampleInputname" class="form-label fw-semibold">BUSCAR TRÁMITE</label>
                                <form class="position-relative">
                                    <input type="text" class="form-control product-search ps-5" id="input-search"
                                        placeholder="">
                                    <i
                                        class="ti ti-search position-absolute top-50 start-0 translate-middle-y fs-6 text-dark ms-3"></i>
                                </form>
                            </div>

                            <div class="col">
                                <div class="form-items2">
                                    <label for="exampleInputname" class="form-label fw-semibold">FECHA INICIAL</label>
                                    <input type="date" name="fecha_ingreso" class="form-control" id="fecha_ingreso"
                                        placeholder="">
                                </div>
                            </div>

                            <div class="col">
                                <div class="form-items2">
                                    <label for="exampleInputname" class="form-label fw-semibold">FECHA FINAL</label>
                                    <input type="date" name="fecha_ingreso" class="form-control" id="fecha_ingreso"
                                        placeholder="">
                                </div>
                            </div>
                        </div>
                        <br>
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered">
                                <thead class="">
                                    <tr>
                                        <th class="text-center align-middle">TRÁMITE</th>
                                        <th class="text-center align-middle">FECHA</th>
                                        <th class="text-center align-middle">PROCESO</th>
                                        <th class="text-center align-middle">CENTRO DE ATENCIÓN</th>
                                        <th class="text-center align-middle">IDENTIFICACIÓN</th>
                                        <th class="text-center align-middle">PROPIETARIO</th>
                                        <th class="text-center align-middle">PLACA</th>
                                        <th class="text-center align-middle">ENTIDAD BANCARIA</th>
                                        <th class="text-center align-middle">COMPROBANTE</th>
                                        <th class="text-center align-middle">FECHA DE PAGO</th>
                                        <th class="text-center align-middle">VALOR</th>
                                        <th class="text-center align-middle">TIPO DE SERVICIO</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% tramites.forEach(tramite=> { %>
                                    <tr>
                                        <td><%= tramite.id_tramite %></td>
                                        <td><%= formatDate(tramite.fecha_finalizacion) %></td>
                                        <td><%= tramite.tipo_tramite %></td>
                                        <td><%= tramite.nombre_corto_empresa %></td>
                                        <td><%= tramite.id_usuario %></td>
                                        <td><%= tramite.nombre_usuario %></td>
                                        <td><%= tramite.placa %></td>
                                        <td><%= tramite.pago_placas_entidad_bancaria %></td>
                                        <td><%= tramite.pago_placas_comprobante %></td>
                                        <td><%= tramite.pago_placas_fecha %></td>
                                        <td><%= tramite.pago_placas_valor %></td>
                                        <td><%= tramite.pago_placas_newservicio %></td>
                                    </tr>
                                    <% }); %>
                                </tbody>
                            </table>
                            




                        </div>
                        <div class="d-flex justify-content-between">
                            <nav aria-label="Page navigation example">
                                <ul class="pagination mb-0">
                                    <li class="page-item">
                                        <a class="page-link link" href="#" aria-label="Previous">
                                            <span aria-hidden="true">
                                                <i class="ti ti-chevrons-left fs-4"></i>
                                            </span>
                                        </a>
                                    </li>
                                    
                                    <% for (let i=1; i <=totalPages; i++) { %>
                                        <li class="page-item">
                                            <a class="page-link link" href="/report-plate?page=<%= i %>">
                                                <%= i %>
                                            </a>
                                        </li>
                                        <% } %>
                                            <li class="page-item">
                                                <a class="page-link link" href="#" aria-label="Next">
                                                    <span aria-hidden="true">
                                                        <i class="ti ti-chevrons-right fs-4"></i>
                                                    </span>
                                                </a>
                                            </li>
                                </ul>
                            </nav>
                            <div class="d-flex justify-content-center gap-2">
                                <div class="text-end">
                                    <button type="submit"
                                        class="justify-content-center btn  btn-rounded btn-secondary align-items-center mb-0  py-2 pt-2">
                                        <i class="ti ti-arrow-narrow-right fs-4 me-2"></i>
                                        <font _mstmutation="1" _msttexthash="76830" _msthash="372"> Procesar consulta
                                        </font>
                                    </button>
                                </div>
                                <br>
                                <div class="text-end  ">
                                    <button type="submit"
                                        class="justify-content-center btn  btn-rounded btn-primary align-items-center mb-0  py-2 pt-2">
                                        <i class="ti ti-file-spreadsheet fs-4 me-2"></i>
                                        <font _mstmutation="1" _msttexthash="76830" _msthash="372"> Exportar a excel
                                        </font>
                                    </button>
                                </div>
                            </div>


                        </div>
                    </div>

                </div>
            </div>

            <div id="spinner-wrapper" class="spinner-wrapper">
                <div class="spinner-border text-primary" role="status">
                    <span class="visually-hidden">Loading..</span>
                </div>
            </div>



        </div>
    </div>

</body>

</html>