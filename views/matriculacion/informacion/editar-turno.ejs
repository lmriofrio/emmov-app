                                                                    placeholder="" required>


                                                                <script>

                                                                    var fecha = new Date();
                                                                    // Obtenemos los componentes de la fecha
                                                                    var year = fecha.getFullYear(); // Año
                                                                    var month = (fecha.getMonth() + 1).toString().padStart(2, '0'); // Mes (se suma 1 porque los meses en JS son de 0-11)
                                                                    var day = fecha.getDate().toString().padStart(2, '0'); // Día

                                                                    // Obtenemos los componentes de la hora
                                                                    var hours = fecha.getHours().toString().padStart(2, '0'); // Hora
                                                                    var minutes = fecha.getMinutes().toString().padStart(2, '0'); // Minutos
                                                                    var seconds = fecha.getSeconds().toString().padStart(2, '0'); // Segundos

                                                                    // Formateamos la fecha como 'YYYY-MM-DD HH:MM:SS'
                                                                    var formattedDate = `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;

                                                                    document.getElementById('fecha_ingreso_INFORMACION').value = formattedDate;
