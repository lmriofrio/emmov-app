                                                        } else {
                                                            inputCambioColor.value = '';
                                                        }

                                                        // Verificar y asignar para Cambio de Servicio
                                                        if (cambioMotorInput.value.trim() === 'SI') {
                                                            inputCambioMotor.value = '- CAMBIO DE MOTOR';
                                                        } else {
                                                            inputCambioMotor.value = '';
                                                        }
                                                    }
                                                    evaluarYAsignarValores();
                                                });

                                            </script>



                                        </div>
