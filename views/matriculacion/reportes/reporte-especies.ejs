<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="/images/favicon.ico" type="image/x-icon">
    <title>Reporte diario</title>
    <link rel="stylesheet" href="../../../public/css/style.css">
    <script src="node_modules/simplebar/dist/simplebar.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@2.46.0/tabler-icons.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.js"
        integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>

    <script src="/js/generar-reporte.js"></script>
    <script src="/js/activar-items-navbar.js"></script>
    <script src="/js/spinners.js"></script>

    <script src="../../../node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>


</head>

<body>

    <div class="page-wrapper" id="main-wrapper" data-theme="blue_theme" data-layout="vertical" data-sidebartype="full"
        data-sidebar-position="fixed" data-header-position="fixed">
        <!-- Inicio de barra de navegación -->
        <%- include('../../body/navbar') %>
            <!-- Fin de barra de navegación -->
            <!--  Contendio principal -->
            <div class="body-wrapper">
                <!--  Encabezado de pagina -->
                <%- include('../../body/header') %>
                    <!--  Fin de encabezado de pagina  -->
                    <div class="container-fluid">
                        <div class="col-lg-12">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">

                                        <div class="d-sm-flex d-block align-items-center justify-content-between mb-9">
                                            <div class="mb-3 mb-sm-0">
                                                <ol class="breadcrumb">
                                                    <li class="breadcrumb-item">
                                                        <a class="text-decoration-none text-black" href="/">REPORTES</a>
                                                    </li>
                                                    <li class="breadcrumb-item text-muted" aria-current="page">
                                                        Reporte de especies
                                                    </li>

                                                </ol>
                                            </div>
                                            <div style="width: 20%;">
                                                <input type="text" name="" class="form-control-disable text-center"
                                                    id="" placeholder="">
                                            </div>
                                        </div>


                                        <input type="hidden" class="form-control" name="jefatura_departamento"
                                            id="jefatura_departamento" value="<%= userData.jefatura_departamento %>"
                                            disabled readonly>

                                        <input type="hidden" class="form-control" name="area_laboral" id="area_laboral"
                                            value="<%= userData.area_laboral %>" disabled readonly>

                                        <input type="hidden" class="form-control" name="rol_funcionario"
                                            id="rol_funcionario" value="<%= userData.rol_funcionario %>" disabled
                                            readonly>

                                        <input type="hidden" class="form-control" name="nombre_funcionario"
                                            id="nombre_funcionario" value="<%= userData.nombre_funcionario %>" disabled
                                            readonly>



                                        <div class="col-12 mb-2 mt-3">
                                            <div class="form-group row">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-normal col-form-label">Nombre del usuario:</label>
                                                <div class="col-md-4">
                                                    <input type="text" class="form-control fw-normald text-uppercase"
                                                        name="nombre_funcionario" id="nombre_funcionario"
                                                        value="<%= userData.nombre_funcionario %>" disabled readonly>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-12 mb-2 d-none">
                                            <div class="form-group row">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-normal col-form-label">Usuario:</label>
                                                <div class="col-md-4">
                                                    <input type="text" class="form-control fw-normal" name="username"
                                                        id="username" value="<%= userData.username %>" disabled
                                                        readonly>
                                                </div>
                                            </div>
                                        </div>




                                        <div class="col-12 mb-2">
                                            <div class="form-group row">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-normal col-form-label">Fecha
                                                    inicial:</label>
                                                <div class="col-md-4">
                                                    <input type="date" class="form-control fw-normal" name="fecha_ingreso"
                                                        id="fecha_ingreso">
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-12 mb-2">
                                            <div class="form-group row">
                                                <label for="area"
                                                    class="col-md-2 form-label  fw-normal col-form-label">Fecha
                                                    final:</label>
                                                <div class="col-md-4">
                                                    <input type="date" class="form-control fw-normal" name="fecha_final"
                                                        id="fecha_final">
                                                </div>
                                            </div>
                                        </div>


                                        <div class="col-12 mb-2">
                                            <div class="form-group row">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-semibold  col-form-label"></label>
                                                <div class="col-md-4 d-flex">
                                                    <button type="submit" class="btn btn-primary me-3"
                                                        id="generarReporteDiarioEspecies">
                                                        <i class="ti ti-search fs-4 me-2"></i>
                                                        <span>Procesar consulta</span>
                                                    </button>

                                                    <form id="loginForms" action="reporte-especies-pdf" method="post"
                                                        target="_blank" style="display: flex;">
                                                        <input type="hidden" class="form-control"
                                                            name="fecha_ingreso_pdf" id="fecha_ingreso_pdf">

                                                        <input type="hidden" class="form-control" name="fecha_final_pdf"
                                                            id="fecha_final_pdf">
                                                        <button type="submit" class="btn btn-secondary me-3" id="">
                                                            <i class="ti ti-file-type-pdf fs-4 me-2"></i>
                                                            <span>Generar PDF</span>
                                                        </button>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>




                                        <div class="col-12 mb-2 d-none">
                                            <div class="form-group row ">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-semibold  col-form-label">Desde la
                                                    fecha:</label>
                                                <div class="col-md-2">
                                                    <input type="hidden" class="form-control text-center"
                                                        name="fecha_ingresada" id="fecha_ingresada">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 mb-2 d-none">
                                            <div class="form-group row ">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-semibold  col-form-label">Hasta la
                                                    fecha:</label>
                                                <div class="col-md-2">
                                                    <input type="hidden" class="form-control text-center"
                                                        name="fecha_ingresada2" id="fecha_ingresada2">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 mb-2 d-none">
                                            <div class="form-group row ">
                                                <label for="area"
                                                    class="col-md-2 form-label fw-semibold  col-form-label">Especies
                                                    utilizadas:</label>
                                                <div class="col-md-2">
                                                    <input type="hidden" class="form-control text-center"
                                                        name="especies_utilizadas" id="especies_utilizadas">
                                                </div>
                                            </div>
                                        </div>


                                        <br>


                                
                                        <div class="col-12 mb-2 d-none" id="content">

                                            <div class="table-responsive">
                                                <table class="table table-bordered">
                                                    <thead class="thead-primary text-black">
                                                        <tr>
                                                            <th
                                                                class="text-center align-middle col-md-10 font-weight-bolder px-2 py-2">
                                                                <b>DETALLE DE LA EMISIÓN DE ESPECIES DE MATRÍCULA</b>
                                                            </th>
                                                            <th class="text-center align-middle col-md-2 px-2 py-2">
                                                                <b>TOTAL</b>
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="tbody-tramites">

                                                    </tbody>

                                                </table>
                                            </div>
                                        </div>





                                    </div>


                                </div>



                            </div>

                        </div>

                    </div>

                    <div id="spinner-wrapper" class="spinner-wrapper">
                        <div class="spinner-border text-primary" role="status">
                            <span class="visually-hidden">Loading..</span>
                        </div>
                    </div>



            </div>

</body>

</html>