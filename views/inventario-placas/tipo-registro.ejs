                              <i class="ti ti-clock-hour-4 fs-4 me-2"></i>En
                              proceso</button>
                          </div>
                        </div>

                      </div>

                      <div class="form-items">
                        <label for="" class="form-label fw-semibold">Placa</label>
                        <input type="text" class="form-control" id="placa" nombre="placa" placeholder=""
                          autocomplete="off">
                      </div>

                      <div class="form-items">
                        <label for="" class="form-label fw-semibold">C. de
                          Vehiculo</label>
                        <select class="form-select" name="clase_vehiculo" aria-label="Default select example"
                          id="clase_vehiculo">
                          <option selected=""></option>
                          <option value="VEHICULO">VEHICULO</option>
                          <option value="MOTOCICLETA">MOTOCICLETA</option>
                        </select>
                      </div>

                      <div class="form-items">
                        <label for="" class="form-label fw-semibold">C. de
                          Transporte</label>
                        <select class="form-select" name="clase_transporte" aria-label="Default select example"
                          id="clase_transporte">
                          <option selected=""></option>
                          <option value="COMERCIAL">COMERCIAL</option>
                          <option value="PARTICULAR">PARTICULAR</option>
                          <option value="PUBLICO">PUBLICO</option>
                          <option value="ESTATAL">ESTATAL</option>
                          <option value="MUNICIPAL">MUNICIPAL</option>
                        </select>
                      </div>


                      <div class="form-items">
                        <label for="" class="form-label fw-semibold">Ubicación</label>
                        <input type="text" class="form-control" id="ubicacion" nombre="ubicacion"
                          aria-label="Default select example">
                      </div>





                      <div>
                        <div class="row justify-content-center align-items-center g-2">
                          <div class="col">
                            <div class="form-items">
                              <label for="exampleInputname" class="form-label fw-semibold">Fecha</label>
                              <input type="date" name="fecha_ingreso" class="form-control" id="fecha_ingreso"
                                placeholder="" required>
                            </div>
                          </div>



                          <script>
                            document.addEventListener('DOMContentLoaded', function () {
                              var fechaUTC = new Date();
                              var desplazamientoNavegador = fechaUTC.getTimezoneOffset();
                              var desplazamientoEcuador = -300;
                              var fechaEcuador = new Date(fechaUTC.getTime() + (desplazamientoEcuador - desplazamientoNavegador) * 60000);
                              var formattedDate = fechaEcuador.toISOString().substr(0, 10);
                              document.getElementById('fecha_ingreso').value = formattedDate;
                            });
                          </script>



                        </div>
                      </div>




                      <div class="text-end mt-4 gap-3">
