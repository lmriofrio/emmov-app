    <div class="body-wrapper">
      <!--  Encabezado de pagina -->
      <header class="app-header">
        <nav class="navbar navbar-expand-lg navbar-light">
        </nav>
      </header>
      <!--  Fin de encabezado de pagina  -->
      <div class="container-fluid" style="width: 60%;">
        <div class="widget-content searchable-container list">

          <div class="row">
            <div class="col-lg-12">


              <form action="/reg-pago-placas" method="POST">
                <input type="hidden" name="id_tramite" value="<%= id_tramite %>">
                <div class="card">
                  <div class="px-4 py-3">
                    <div class="card-body p-0">
                      <div class="d-sm-flex d-block align-items-center justify-content-between mb-9">

                        <div class="mb-3 mb-sm-0">
                          <h5 class="card-title fw-semibold">Registro del comprobante de pago</h5>
                          <p class="card-subtitle mb-0">Información del comprobante de pago</p>
                        </div>

                        <div class="ms-auto">
                          <div class="btn-group">
                            <button type="button" class="btn bg-secondary-subtle text-secondary font-medium"
                              data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              <i class="ti ti-clock-hour-4 fs-4 me-2"></i>En
                              proceso</button>
                          </div>
                        </div>

                      </div>
                    </div>



                    <div class="alert bg-primary-subtle text-info alert-dismissible fade show" role="alert">
                      <div class="d-flex align-items-center text-primary font-medium">
                        <i class="ti ti-info-circle me-2 fs-4"></i>
                        Se ingresa los datos del comprobante de pago de placas de motos y vehiculos
                      </div>
                      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                    </div>



                    <div class="px-5 py-3">
                      <div class="row">

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">Placa:</label>
                            <div class="col-md-7">
                              <p class="form-control-static">
                                <%= placa %>
                              </p>
                            </div>
                          </div>
                        </div>

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">C.I./RUC:</label>
                            <div class="col-md-7">
                              <p class="form-control-static">
                                <%= id_usuario %>
                              </p>
                            </div>
                          </div>
                        </div>

                      </div>

                      <div class="row">

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">Clase de vehiculo:</label>
                            <div class="col-md-7">
                              <p class="form-control-static">
                                <%= clase_vehiculo %>
                              </p>
                            </div>
                          </div>
                        </div>

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">Propietario:</label>
                            <div class="col-md-7">
                              <div style="white-space: nowrap; overflow: hidden;">
                                <p class="form-control-static">
                                  <%= nombre_usuario %>
                                </p>
                              </div>
                            </div>
                          </div>
                        </div>




                      </div>

                      <div class="row">

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">Clase de transporte:</label>
                            <div class="col-md-7">
                              <p class="form-control-static">
                                <%= clase_transporte %>
                              </p>
                            </div>
                          </div>
                        </div>

                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="form-label text-start col-md-5">Tipo de trámite:</label>
                            <div class="col-md-7" style="white-space: nowrap; overflow: hidden;">
                              <p class="form-control-static">
                                <%= tipo_tramite %>
                              </p>
                            </div>
                          </div>
                        </div>

                      </div>

                    </div>

                    <h5 class="fs-4 fw-semibold">Comprobante de pago:</h5>

                    <div class="py-3">


                      <div class="row">
                        <div class="col-lg-6">

                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">Entidad bancaria</label>
                            <input type="text" name="pago_placas_entidad_bancaria" class="form-control"
                              id="pago_placas_entidad_bancaria" placeholder="Nombre del banco o cooperativa"
                              autocomplete="off">
                          </div>

                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">Fecha de pago</label>
                            <input type="date" name="pago_placas_fecha" class="form-control" id="pago_placas_fecha"
                              placeholder="Fecha de pago" autocomplete="off">
                          </div>

                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">Nº trámite AXIS</label>
                            <input type="text" name="id_tramite_axis" class="form-control" id="id_tramite_axis"
                              placeholder="Número de tramite en el sistema Axis" autocomplete="off">
                          </div>
                        </div>

                        <div class="col-lg-6">

                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">N° Comprobante</label>
                            <input type="number" name="pago_placas_comprobante" class="form-control"
                              id="pago_placas_comprobante" placeholder="Número del comprobante" autocomplete="off">
                          </div>

                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">Valor</label>

                            <select class="form-select" name="pago_placas_valor" aria-label="Default select example"
                              id="pago_placas_valor" placeholder="0" autocomplete="off">
                              <option selected="0"></option>
                              <option value="23.00">23.00</option>
                              <option value="13.00">13.00</option>
                            </select>


                          </div>
                          <div class="mb-4">
                            <label for="" class="form-label fw-semibold">Nuevo tipo servicio</label>
                            <input type="text" name="pago_placas_newservicio" class="form-control" id="pago_placas_newservicio"
                             placeholder="" autocomplete="off" value="<%= clase_transporte %>">
                          </div>

                        </div>
                      </div>

                    </div>


                    <div class="form-actions">
                      <div class="card-body px-0 py-0 d-flex justify-content-end">
                        <button type="submit"
                          class="justify-content-center btn mb-1 btn-rounded btn-primary align-items-center">
                          <i class="ti ti-device-floppy fs-4 me-2"></i>
                          <font _mstmutation="1" _msttexthash="76830" _msthash="372"> Guardar </font>
                        </button>
                      </div>
                    </div>


                  </div>






              </form>



            </div>







          </div>

        </div>
      </div>



    </div>


  </div>

  </div>
  </div>

  <div id="spinner-wrapper" class="spinner-wrapper">
    <div class="spinner-border text-primary" role="status">
      <span class="visually-hidden">Loading..</span>
    </div>
  </div>


  </div>
  </div>


  <script src="https://cdn.jsdelivr.net/npm/@tabler/icons@latest/dist/tabler-icons.min.js"></script>
</body>

</html>